#include <iostream>

using namespace std;

int main()
{
    int num;
    int mul;
    
    cout<<"Enter a number: ";
    cin>>num;
    cout<<"This is the multiplication of "<<num<<": "<<endl;
    for(mul=1; mul<=10; mul++)
    {
        cout<<mul<<" * "<<num<<" = "<<mul*num<<endl;
    }
    
    return 0;
    
}
